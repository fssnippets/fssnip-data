<span class="c">//  &quot;{&quot;$case&quot;:&quot;Node&quot;,&quot;value&quot;:3,&quot;left&quot;:{&quot;$case&quot;:&quot;Node&quot;,&quot;value&quot;:1,&quot;left&quot;:&quot;Leaf&quot;,&quot;right&quot;:{&quot;$case&quot;:&quot;Node&quot;,&quot;value&quot;:2,&quot;left&quot;:&quot;Leaf&quot;,&quot;right&quot;:&quot;Leaf&quot;}},&quot;right&quot;:&quot;Leaf&quot;}&quot;</span>
<span class="id">serialize</span> <span class="id">p4</span> <span class="pn">(</span><span class="id">Node</span><span class="pn">(</span><span class="n">3</span><span class="pn">,</span> <span class="id">Node</span><span class="pn">(</span><span class="n">1</span><span class="pn">,</span> <span class="id">Leaf</span><span class="pn">,</span> <span class="id">Node</span><span class="pn">(</span><span class="n">2</span><span class="pn">,</span> <span class="id">Leaf</span><span class="pn">,</span><span class="id">Leaf</span><span class="pn">)</span><span class="pn">)</span><span class="pn">,</span> <span class="id">Leaf</span><span class="pn">)</span><span class="pn">)</span>
