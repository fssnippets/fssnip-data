<span class="k">let</span> <span class="id">p3</span> <span class="o">=</span> <span class="id">generateConverter</span><span class="pn">&lt;</span><span class="id">Bar</span> <span onmouseout="hideTip(event, 'fs52', 169)" onmouseover="showTip(event, 'fs52', 169)" class="id">list</span> <span class="pn">[</span><span class="pn">]</span><span class="pn">&gt;</span><span class="pn">(</span><span class="pn">)</span>

<span class="c">// &quot;[[],[&quot;C&quot;,{&quot;$case&quot;:&quot;D&quot;,&quot;item&quot;:null}],[{&quot;$case&quot;:&quot;Foo&quot;,&quot;values&quot;:{&quot;A&quot;:42,&quot;B&quot;:&quot;12&quot;}}],[{&quot;$case&quot;:&quot;E&quot;,&quot;items&quot;:{&quot;foo&quot;:42}}]]&quot;</span>
<span class="id">serialize</span> <span class="id">p3</span> <span class="pn">[|</span> <span class="pn">[</span><span class="pn">]</span> <span class="pn">;</span> <span class="pn">[</span><span class="id">C</span> <span class="pn">;</span> <span class="id">D</span> <span onmouseout="hideTip(event, 'fs49', 170)" onmouseover="showTip(event, 'fs49', 170)" class="id">None</span><span class="pn">]</span> <span class="pn">;</span> <span class="pn">[</span><span class="id">Foo</span> <span class="pn">{|</span> <span class="id">A</span> <span class="o">=</span> <span class="n">42</span> <span class="pn">;</span> <span class="id">B</span> <span class="o">=</span> <span class="s">&quot;12&quot;</span> <span class="pn">|}</span><span class="pn">]</span> <span class="pn">;</span> <span class="pn">[</span><span class="id">E</span> <span class="pn">(</span><span onmouseout="hideTip(event, 'fs56', 171)" onmouseover="showTip(event, 'fs56', 171)" class="id">Map</span><span class="pn">.</span><span onmouseout="hideTip(event, 'fs65', 172)" onmouseover="showTip(event, 'fs65', 172)" class="id">ofList</span> <span class="pn">[</span><span class="pn">(</span><span class="s">&quot;foo&quot;</span><span class="pn">,</span> <span class="n">42</span><span class="pn">)</span><span class="pn">]</span><span class="pn">)</span><span class="pn">]</span><span class="pn">|]</span>
<span class="o">|&gt;</span> <span class="id">deserialize</span> <span class="id">p3</span>

<span class="c">// Recursive type serialization</span>
<span class="k">type</span> <span class="id">BinTree</span><span class="pn">&lt;</span><span class="id">&#39;</span><span class="id">T</span><span class="pn">&gt;</span> <span class="o">=</span> <span class="id">Leaf</span> <span class="pn">|</span> <span class="id">Node</span> <span class="k">of</span> <span class="id">value</span><span class="pn">:</span><span class="id">&#39;</span><span class="id">T</span> <span class="pn">*</span> <span class="id">left</span><span class="pn">:</span><span class="id">BinTree</span><span class="pn">&lt;</span><span class="id">&#39;</span><span class="id">T</span><span class="pn">&gt;</span> <span class="pn">*</span> <span class="id">right</span><span class="pn">:</span><span class="id">BinTree</span><span class="pn">&lt;</span><span class="id">&#39;</span><span class="id">T</span><span class="pn">&gt;</span>

