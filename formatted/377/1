    <span class="c">// Spawn the crawlers.</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs93', 295)" onmouseover="showTip(event, 'fs93', 295)" class="id">crawlers</span> <span class="o">=</span> 
        <span class="pn">[</span>
            <span class="k">for</span> <span onmouseout="hideTip(event, 'fs94', 296)" onmouseover="showTip(event, 'fs94', 296)" class="id">i</span> <span class="k">in</span> <span class="n">1</span> <span class="o">..</span> <span onmouseout="hideTip(event, 'fs21', 297)" onmouseover="showTip(event, 'fs21', 297)" class="id">Gate</span> <span class="k">do</span>
