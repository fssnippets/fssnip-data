                            <span class="pn">|</span> <span class="id">_</span> <span class="k">-&gt;</span> <span class="k">return!</span> <span onmouseout="hideTip(event, 'fs82', 234)" onmouseover="showTip(event, 'fs82', 234)" class="fn">loop</span> <span class="pn">(</span><span onmouseout="hideTip(event, 'fs75', 235)" onmouseover="showTip(event, 'fs75', 235)" class="id">count</span> <span class="o">+</span> <span class="n">1</span><span class="pn">)</span>
                    <span class="pn">|</span> <span onmouseout="hideTip(event, 'fs46', 236)" onmouseover="showTip(event, 'fs46', 236)" class="uc">None</span> <span class="k">-&gt;</span> <span onmouseout="hideTip(event, 'fs66', 237)" onmouseover="showTip(event, 'fs66', 237)" class="fn">supervisor</span><span class="pn">.</span><span onmouseout="hideTip(event, 'fs29', 238)" onmouseover="showTip(event, 'fs29', 238)" class="id">Post</span> <span onmouseout="hideTip(event, 'fs13', 239)" onmouseover="showTip(event, 'fs13', 239)" class="uc">Stop</span>
