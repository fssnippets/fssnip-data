                <span class="pn">}</span>
            <span onmouseout="hideTip(event, 'fs82', 242)" onmouseover="showTip(event, 'fs82', 242)" class="fn">loop</span> <span class="n">1</span><span class="pn">)</span>
    
    <span class="c">/// Initializes a crawling agent.</span>
    <span class="k">let</span> <span onmouseout="hideTip(event, 'fs86', 243)" onmouseover="showTip(event, 'fs86', 243)" class="fn">crawler</span> <span onmouseout="hideTip(event, 'fs87', 244)" onmouseover="showTip(event, 'fs87', 244)" class="id">id</span> <span class="o">=</span>
        <span onmouseout="hideTip(event, 'fs12', 245)" onmouseover="showTip(event, 'fs12', 245)" class="d">MailboxProcessor</span><span class="pn">.</span><span onmouseout="hideTip(event, 'fs57', 246)" onmouseover="showTip(event, 'fs57', 246)" class="id">Start</span><span class="pn">(</span><span class="k">fun</span> <span onmouseout="hideTip(event, 'fs88', 247)" onmouseover="showTip(event, 'fs88', 247)" class="id">inbox</span> <span class="k">-&gt;</span>
            <span class="k">let</span> <span class="k">rec</span> <span onmouseout="hideTip(event, 'fs89', 248)" onmouseover="showTip(event, 'fs89', 248)" class="fn">loop</span><span class="pn">(</span><span class="pn">)</span> <span class="o">=</span>
                <span onmouseout="hideTip(event, 'fs59', 249)" onmouseover="showTip(event, 'fs59', 249)" class="k">async</span> <span class="pn">{</span>
